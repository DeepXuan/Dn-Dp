{
    "name": "sr_Liver_128_512",
    "phase": "val", // train or val
    "gpu_ids": [
        0
    ],
    "path": { //set the path
        "resume_state": "./Pretrained/Liver_128_512"
    },
    "model": {
        "which_model_G": "sr3", // use the ddpm or sr3 network structure
        "mean_type": "epsilon",
        "finetune_norm": false,
        "unet": {
            "in_channel": 2, //6
            "out_channel": 1, //3
            "inner_channel": 64,
            "norm_groups": 16,
            "channel_multiplier": [
                1,
                2,
                4,
                8,
                16,
                32,
                32
            ],
            "attn_res": [
                // 16, 32
            ],
            "res_blocks": 2,
            "dropout": 0,
            "patch_n": 1,
            "patch_n_cond": 1
        },
        "beta_schedule": { // use munual beta_schedule for acceleration
            "train": {
                "schedule": "linear",
                "n_timestep": 2000,
                "linear_start": 1e-6,
                "linear_end": 1e-2
            },
            "val": {
                "schedule": "linear",
                "n_timestep": 2000, //2000
                "linear_start": 1e-6,
                "linear_end": 1e-2
            }
        },
        "diffusion": {
            "image_size": 512,
            "channels": 1, //sample channel
            "conditional": true // unconditional generation or conditional generation(super_resolution)
        }
    }
}
